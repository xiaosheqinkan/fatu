{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth-url",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/callback",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/repost-tweet",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}